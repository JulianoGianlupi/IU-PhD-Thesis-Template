	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/adhm.202000979},
	keywords = {COVID-19, coronavirus, diagnosis, nanomaterials, nanotechnology, treatment, vaccines},
	pages = {2000979},
}

