	file = {ScienceDirect Full Text PDF:C\:\\Users\\julia\\Zotero\\storage\\4X6PSADP\\Swat et al. - 2012 - Chapter 13 - Multi-Scale Modeling of Tissues Using.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\julia\\Zotero\\storage\\ELLM2U77\\B9780123884039000138.html:text/html},
