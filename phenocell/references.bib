	keywords = {artificial intelligence medical system, Bioinformatics, Costs, drug effectiveness-cost analysis, Drugs, Lung cancer, Medical diagnostic imaging, Non-small cell lung cancer (NSCLC), synthetic biology, Synthetic biology, targeted drug therapy, Tumors},
	pages = {5055--5066},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\julia\\Zotero\\storage\\7ISNYCZV\\9640494.html:text/html;IEEE Xplore Full Text PDF:C\:\\Users\\julia\\Zotero\\storage\\FH5FT8P4\\Chang et al. - 2022 - AI-Driven Synthetic Biology for Non-Small Cell Lun.pdf:application/pdf},
